		<dict>
			<key>MainClass</key>
			<string>com.github.yoep.popcorn.PopcornTimeStarter</string>
			<key>ClassPath</key>
			<array>
				<string>Java/popcorn-time.jar</string>
			</array>
			<key>VMOptions</key>
			<array>
				<string>-Djna.library.path="../Resources/:$PATH"</string>
